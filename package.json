{
  "name": "landing",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "nodemon --watch src --ext js,ts,json --exec node src/server.js",
    "start": "node src/server.js",
    "build": "npm run prisma:generate",
    "postinstall": "npm run prisma:generate",
    "prisma:generate": "prisma generate",
    "prisma:generate:dev": "prisma generate --schema=./prisma/schema.dev.prisma",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:db:push": "prisma db push",
    "db:setup": "prisma db push --accept-data-loss"
  },
  "dependencies": {
    "@prisma/client": "^6.13.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "nodemon": "^3.1.10",
    "prisma": "^6.13.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
